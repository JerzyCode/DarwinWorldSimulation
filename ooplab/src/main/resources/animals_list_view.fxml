<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="agh.ics.oop.presenter.AnimalListView"
            prefHeight="300.0" prefWidth="200.0">
    <VBox spacing="10.0" style="-fx-padding: 10;" prefWidth="200.0">
        <Label style="-fx-font-weight: bold; -fx-font-size: 24;" text="Select Animal "/>
        <Label style="-fx-font-weight: bold; -fx-font-size: 16;" text="Animals At Position: "/>
        <ListView fx:id="animalCells" prefHeight="500.0" />
        <Button fx:id="cancelButton" text="Cancel" style="-fx-font-size: 14;"/>
    </VBox>
</AnchorPane>
